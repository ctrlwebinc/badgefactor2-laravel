{
    "name": "ctrlwebinc/laravel-badgefactor2",
    "description": "A marvellous bridge between Laravel and Badgr.",
    "keywords": [
      "laravel",
      "badgr",
      "open-badge"
    ],
    "license": "MIT",
    "authors": [
      {
        "name": "Daniel Le Blanc",
        "email": "daniel@ctrlweb.ca"
      }
    ],
    "require": {
        "php": "^8.0.2",
        "bordoni/phpass": "dev-main",
        "doctrine/dbal": "^2.9"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
      "psr-4": {
        "Ctrlweb\\BadgeFactor2\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Ctrlweb\\BadgeFactor2\\Tests\\": "tests/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Ctrlweb\\BadgeFactor2\\CoreServiceProvider"
        ],
        "aliases": {
          "BadgeFactor2": "Ctrlweb\\BadgeFactor2\\BadgeFactor2"
        }
      }
    },
    "config": {
      "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "scripts": {
    },
    "repositories": [
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "0.0.2"
  }
